{
  "name": "frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "Fastify-based frontend service",
  "main": "src/index.ts",
  "scripts": {
  "dev:ts": "tsc --watch --project tsconfig.json",
  "dev:sass": "sass --watch src/scss:public/css",
  "dev:vite": "vite --host --port 5173",
  "dev": "concurrently -k -n TS,SASS,VITE -c yellow,cyan,magenta \"npm run dev:ts\" \"npm run dev:sass\" \"npm run dev:vite\"",
  "build": "tsc -p tsconfig.json",
  "build-client": "sass src/scss:public/css && vite build",
  "build-all": "npm run build && npm run build-client",
  "start": "vite preview --port 5173 --strictPort"
},
  "dependencies": {
    "mathjs": "14.5.3",
    "fastify": "^4.22.2",
    "@fastify/static": "^6.12.0",
    "dotenv": "^16.4.5",
    "socket.io-client": "^4.8.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "typescript": "^5.4.5",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "sass": "^1.89.2",
    "vite": "^7.1.1",
    "dotenv": "^16.4.5"
  }
}
